# Keep Alive
ServerAliveInterval 120

Host aur aur.archlinux.org
  IdentityFile ~/.ssh/aur
  Hostname aur.archlinux.org
  User aur

Host github.com                                                                                                             
  User git
  # socks5 proxy
  ProxyCommand nc -x localhost:1089 %h %p
  # http proxy
  #ProxyCommand socat - PROXY:127.0.0.1:%h:%p,proxyport=8889
