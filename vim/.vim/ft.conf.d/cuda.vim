vim9script
# vim: set sw=4 ts=8 sts=4 et foldmethod=marker:
# kate: space-indent on; indent-width 4; syntax vim;

# Enable highlighting of CUDA kernel calls
g:cuda_kernel_highlight = 1
# Highlight keywords from CUDA Runtime API
g:cuda_runtime_api_highlight = 1
# Highlight keywords from CUDA Driver API
g:cuda_driver_api_highlight = 1
# Highlight keywords from CUDA Thrust library
g:cuda_thrust_highlight = 1
